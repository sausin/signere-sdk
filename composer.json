{
    "name": "sausin/signere-sdk",
    "description": "Package to work with Signere.no APIs for bankid implementation.",
    "keywords": ["laravel", "signere", "api"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/sausin/signere-sdk/issues",
        "source": "https://github.com/sausin/signere-sdk"
    },
    "authors": [
        {
            "name": "Saurabh Singhvi",
            "email": "saurabh.singhvi@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/contracts": "~5.4",
        "illuminate/routing": "~5.4",
        "illuminate/support": "~5.4",
        "illuminate/validation": "~5.4",
        "jakeasmith/http_build_url": "^1.0",
        "nesbot/carbon": "~1.0"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~5.0",
        "orchestra/testbench": "~3.4",
        "spatie/phpunit-watcher": "^1.3"
    },
    "autoload": {
        "files": [
            "src/Support/helpers.php"
        ],
        "psr-4": {
            "Sausin\\Signere\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sausin\\Signere\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sausin\\Signere\\SignereServiceProvider"
            ],
            "aliases": {
                "EventsFacade": "Sausin\\Signere\\Facades\\EventsFacade",
                "HeadersFacade": "Sausin\\Signere\\Facades\\HeadersFacade",
                "StatusFacade": "Sausin\\Signere\\Facades\\StatusFacade"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
